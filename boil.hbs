{
  "bin":{
    "dest": "\{{args.[0]}}/bin/cli.js",
    "template": {{{json-stringify (fs-readFile "assets/bin/cli.js")}}}
  },
  "jsdoc2md":{
    "dest": "\{{args.[0]}}/jsdoc2md/README.hbs",
    "template": {{{json-stringify (fs-readFile "assets/jsdoc2md/README.hbs")}}}
  },
  "lib":{
    "dest": "\{{args.[0]}}/lib/\{{args.[0]}}.js",
    "template": {{{json-stringify (fs-readFile "assets/lib/main.js")}}}
  },
  "test":{
    "dest": "\{{args.[0]}}/test/test.js",
    "template": {{{json-stringify (fs-readFile "assets/test/test.js")}}}
  },
  "package.json":{
    "dest": "\{{args.[0]}}/package.json",
    "template": {{{json-stringify (fs-readFile "assets/package.json")}}}
  },
  "LICENSE":{
    "dest": "\{{args.[0]}}/LICENSE",
    "template": {{{json-stringify (fs-readFile "assets/LICENSE")}}}
  },
  "readme":{
    "dest": "\{{args.[0]}}/README.md",
    "template": {{{json-stringify (fs-readFile "assets/README.md")}}}
  },
  "travis":{
    "dest": "\{{args.[0]}}/.travis.yml",
    "template": {{{json-stringify (fs-readFile "assets/.travis.yml")}}}
  },
  "main":{
    "dest": "\{{args.[0]}}/.main.js",
    "template": {{{json-stringify (fs-readFile "assets/main.js")}}}
  },
  "package": [ "bin", "jsdoc2md", "lib", "test", "LICENSE", "package.json", "readme", "travis", "main" ]
}
